{
  "name": "heroku-cli-util",
  "description": "Set of helpful CLI utilities",
  "version": "5.10.10",
  "author": "Jeff Dickey (@dickeyxxx)",
  "bugs": {
    "url": "https://github.com/heroku/heroku-cli-util/issues"
  },
  "dependencies": {
    "cardinal": "0.7.1",
    "chalk": "1.1.3",
    "cli-cursor": "1.0.2",
    "co": "4.6.0",
    "got": "6.3.0",
    "heroku-client": "2.4.3",
    "lodash": "4.11.1",
    "opn": "3.0.3",
    "supports-color": "3.1.2",
    "tunnel-agent": "0.4.2"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "changelog": "1.0.7",
    "codeclimate-test-reporter": "0.3.1",
    "coveralls": "2.11.9",
    "hook-std": "0.2.0",
    "jsdoc": "*",
    "mocha": "^2.2.4",
    "nock": "8.0.0",
    "nyc": "6.4.0",
    "proxyquire": "1.7.4",
    "sinon": "1.17.3",
    "snazzy": "3.0.1",
    "standard": "6.0.8",
    "unexpected": "10.13.2"
  },
  "homepage": "https://github.com/heroku/heroku-cli-util",
  "license": "ISC",
  "main": "index.js",
  "repository": "heroku/heroku-cli-util",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | codeclimate-test-reporter && nyc report --reporter=text-lcov | coveralls",
    "doc": "jsdoc lib",
    "postversion": "npm publish && git push && git push --tags",
    "preversion": "npm test && changelog heroku-cli-util all -m > CHANGELOG.md && git add CHANGELOG.md",
    "test": "nyc mocha && standard | snazzy"
  },
  "standard": {
    "ignore": [
      "lib/linewrap.js"
    ]
  },
  "nyc": {
    "exclude": [
      "lib/linewrap.js"
    ]
  }
